{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${version}",
  "name": "${display_name}",
  "description": "${description}",
  "authors": [
    "${authors}"
  ],
  "contact": {
    "homepage": "${homepage}",
    "issues": "${issues}",
    "sources": "${source}"
  },
  "license": "${license}",
  "icon": "${icon}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "${maven_group}.fabric.${display_name}Fabric"
    ]
  },
  "mixins": [
    "${modid}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=${java_version}",
    "dragonlib": ">=${dragonlib_version}",
    "create": "*",
    "fabric-api": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Discord": "${discord}"
      },
      "update_checker": true
    }
  },
  "accessWidener": "${modid}.accesswidener"
}
